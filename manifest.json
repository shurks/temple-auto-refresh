{
    "manifest_version": 2,
    "name": "Auto Reload",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage",
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }